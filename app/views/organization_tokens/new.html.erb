
<% if @token.errors.any? %>
  <% @token.errors.full_messages.each do |message|%>\
    <h3><%= message %> </h3>
  <%end%>
<%end%>

<%= form_with model: [@organization, @token] do |form| %>
  <%= form.text_field :name %>
  <%= form.text_field :scopes %>
  <%= form.text_field :expires_at %>
  <%= form.submit %>
<% end %>